<script>
	window.top.window.refreshHandle();
	function refreshHandle(){
		$('#uploadInput').val('');
		$('#nameFolder').val('');
		$.get({link Refresh!});
	};

</script>
<div class="Kappa-fileManager">
	<div id="nahravac_pozadi"></div>
{snippet Kappa-fileManager}
	<div id="nahravac">
		<div class="nahravac_hlavicka">
			<h1>Správce souborů a složek</h1>
			<a href="#" class="zavrit Kappa-fileManager-close">Zavřít Správce</a>
		</div>
		<div id="nahravac_leva">
			<div class="nahravac_hlavicka_strana"> Aktuálně vybraný soubor </div>
			<br />
			<img src="data:image/png;base64," class="nahravac_nahled" alt=""  />
			<p class="clear"></p>
			<p>Název souboru:<br /><span id="fileName"></span></p>
			<form action="#" method="post" id="actionForm" onsubmit="return false;">
				<p>

					<label>Zobrazit jako:</label>
					<select id="action">
						<option value="img">Náhled</option>
						<option value="ahref">Odkaz</option>
					</select>

				</p>
				<p>
					<label>Text:</label>
					<input class="nahravac_input_text" id="text" type="text" />

				</p>
				<input type="submit" value="Přidat na stránku" class="nahravac_pridat"/>
			</form>
		</div>
		<div id="nahravac_prava">
			<div class="nahravac_hlavicka_strana">
				<ul>
					<li><a n:href="Move 'home'" class="ajax">upload</a> /</li>
					<li n:foreach="$navigation as $path"><a n:href="Move $path" class="ajax">{$path}</a> /</li>
				</ul>
			</div>
			<ul id="nahravac_popisek">
				<li class="a"><input type="checkbox" id="allCheck"><a href="">Název</a></li>
				<li class="c"><a href="">Datum přidání</a></li>
				<li class="d"><a href="">Smazat</a></li>
			</ul>
			<div id="nahravac_vnitrek">
				<ul class="nahravac_slozky_a_soubory" n:foreach="$directories as $dir">
					<li class="a"><a n:href="Move $dir['name']" class="ajax"><img src="{$basePath}/assets/images/ikony/slozka.png" alt="" />{$dir['name']}</a></li>
					<li class="c">{$dir['date']}</li>
					<li class="d"><a n:href="DeleteDir $dir['path']" class="ajax"><img src="{$basePath}/assets/images/ikony/smazat.png" alt="Smazat" /></a></li>
				</ul>
				<ul class="nahravac_slozky_a_soubory" n:foreach="$files as $i => $file">
					<li class="a"><img src="{$basePath}/assets/images/ikony/{$file['icon']}.png" alt="" />
						<input type="checkbox" value="true" class="file-checkbox" id="file-checkbox-{$i}" data-type="{$file['icon']}" data-name="{$file['name']}" data-path="{$basePath}/{$file['path']}"/>
						<a href="">{$file['name']}</a></li>
					<li class="c">{$file['date']}</li>
					<li class="d"><a n:href="DeleteFile $file['path']" class="ajax"><img src="{$basePath}/assets/images/ikony/smazat.png" alt="Smazat" /></a></li>
				</ul>

			</div>
			<div class="nahravac_dole">
				{form addNewDir target=>upload_target, id=>uploadForm}
						{label name /}
						{input name}
						{input submit}
					{/form}
			</div>
			<div class="nahravac_dole">
				{form addNewFile target=>upload_target, id=>uploadForm}
						{input files}
						{input submit class=>"nahrat_vytvorit"}
					{/form}
			</div>
		</div>
	</div>
{/snippet}
	<iframe id="upload_target" name="upload_target" src="#" style="display: none;"></iframe>
</div>

