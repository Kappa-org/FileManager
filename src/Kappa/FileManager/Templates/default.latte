{snippet Kappa-fileManager}
<div id="filemanager">
	<nav class="navbar navbar-inverse navbar-static-top" role="navigation">
		<div class="navbar-header">
			<a class="navbar-brand" href="#">Správce souborů</a>
		</div>
	</nav>
	<div id="body">
		<div class="info-panel">
			<img id="image-preview" src="http://placehold.it/200x200" width="200" height="200">
			<h2>Vložení do stránky:</h2>
			<hr>
			<form action="#" id="insert-form">
				<table>
					<tr>
						<td>Vložit jako</td>
					</tr>
					<tr>
						<td>
							<select data-filemanager="type" class="form-control">
								<option value="img">Obrázek</option>
								<option value="link">Odkaz</option>
							</select>
						</td>
					</tr>
					<tr>
						<td>Text:</td>
					</tr>
					<tr>
						<td><input type="text" data-filemanager="text" class="form-control"></td>
					</tr>
					<tr>
						<td>
							<input type="submit" class="btn btn-block btn-primary" value="Vložit do stránky">
						</td>
					</tr>
				</table>
			</form>
		</div>
		<div class="content">
			<ul class="breadcrumb">
				<li><a n:href="Move! 'home'" class="ajax">home</a></li>
				<li n:foreach="$navigation as $path"><a n:href="Move! $path" class="ajax">{$path}</a></li>
			</ul>
			<div class="bs-docs-example">
				<ul id="myTab" class="nav nav-tabs">
					<li class="active"><a href="#list" data-toggle="tab">Přehled souborů</a></li>
					<li><a href="#folder" data-toggle="tab">Nová složka</a></li>
					<li><a href="#files" data-toggle="tab">Nové soubory</a></li>
				</ul>
				<div id="myTabContent" class="tab-content">
					<div class="tab-pane fade in active" id="list">
						<table class="table">
							<thead>
							<tr>
								<th class="check"><input type="checkbox" id="check-all"></th>
								<th class="icon"></th>
								<th>Jméno</th>
								<th class="actions">Akce</th>
							</tr>
							</thead>
							<tbody id="list-checkboxs">
							<tr n:foreach="$directories as $directory">
								<td class="input">
									<input type="checkbox"{if $openType == "files"} disabled{/if} data-src="{$directory->getInfo()->getRelativePath($basePath)}">
								</td>
								<td class="icon">
									<img alt="" src="data:image/png;base64," />
								</td>
								<td>
									<a n:href="Move! $directory->getInfo()->getBasename()" class="ajax">{$directory->getInfo()->getBasename()}</a>
								</td>
								<td class="button-group">
									<a n:href="DeleteDir! $directory->getInfo()->getPathname()" class="btn btn-danger btn-xs ajax" data-toggle="tooltip" data-placement="left" data-original-title="Remove directory">x</a>

								</td>
							</tr>
							<tr n:foreach="$files as $file">
								<td class="input">
									<input type="checkbox" class="img-checkbox"{if $openType == "directories"} disabled{/if} data-src="{$file->getInfo()->getRelativePath($basePath)}" data-type="{if $file->getInfo()->isImage()}image{else}other{/if}">
								</td>
								<td class="icon">
									<img alt="" src="data:image/png;base64," />
								</td>
								<td>{$file->getInfo()->getBasename()}</td>
								<td class="button-group">
										<a n:href="DeleteFile! $file->getInfo()->getPathname()" class="btn btn-danger btn-xs ajax" data-toggle="tooltip" data-placement="left" data-original-title="Remove file">x</a>
								</td>
							</tr>
							</tbody>
						</table>
					</div>
					<div class="tab-pane fade in" id="folder">
						{form directory}
							<table class="table table-bordered">
								<tr>
									<td >{label name /}</td>
									<td>{input name class=>'form-control'}</td>
								</tr>
								<tr>
									<td></td>
									<td>{input submit}</td>
								</tr>
							</table>
						{/form}
					</div>
					<div class="tab-pane fade" id="files">
						{form file}
							<table class="table table-bordered">
								<tr>
									<td >{label files /}</td>
									<td>{input files class=>'form-control'}</td>
								</tr>
								<tr>
									<td></td>
									<td>{input submit}</td>
								</tr>
							</table>
						{/form}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
{/snippet}
